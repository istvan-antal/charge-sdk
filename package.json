{
  "name": "charge-sdk",
  "version": "1.4.0",
  "description": "Build, run and test tools for React projects written in TypeScript.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "tsc --noEmit && eslint --ext ts,tsx --quiet ./src && bash -c \"babel-node --extensions '.ts' ./src/cli.ts test\"",
    "build": "bash -c \"tsc --declaration\"",
    "prepare": "npm run build",
    "postversion": "git push --tags && git push"
  },
  "engines": {
    "node": "13.7.0",
    "npm": "6.13.6"
  },
  "bin": {
    "charge-sdk": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istvan-antal/charge-sdk.git"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Istv√°n Antal <i@istvan.xyz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/istvan-antal/charge-sdk/issues"
  },
  "homepage": "https://github.com/istvan-antal/charge-sdk#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-transform-react-jsx",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ],
      "@babel/plugin-transform-modules-commonjs",
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^7.1.0",
    "jest": "^25.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss": "^8.2.10",
    "postcss-hexrgba": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-initial": "^3.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-math": "0.0.10",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^4.2.1",
    "postcss-simple-vars": "^5.0.2",
    "react-dev-utils": "9.1.0",
    "read-cache": "^1.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.2.0",
    "typescript": "^3.8.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.1",
    "webpack-dev-server": "^3.10.3"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.1",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^13.13.0",
    "@types/postcss-nested": "^4.1.0",
    "@types/react-dev-utils": "^9.0.2",
    "@types/webpack-dev-server": "^3.10.1"
  }
}
